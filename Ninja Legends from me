Unlockallelements.Name = "Unlock all elements"
Unlockallelements.Parent = Frame
Unlockallelements.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
Unlockallelements.Position = UDim2.new(0.0188679248, 0, 0.646090508, 0)
Unlockallelements.Size = UDim2.new(0, 89, 0, 50)
Unlockallelements.Font = Enum.Font.LuckiestGuy
Unlockallelements.Text = "Unlock All Elements"
Unlockallelements.TextColor3 = Color3.fromRGB(255, 0, 0)
Unlockallelements.TextScaled = true
Unlockallelements.TextSize = 14.000
Unlockallelements.TextWrapped = true
Unlockallelements.MouseButton1Down:connect(function()
	--Frost
	game.ReplicatedStorage.rEvents.elementMasteryEvent:FireServer("Frost")

	--Inferno
	game.ReplicatedStorage.rEvents.elementMasteryEvent:FireServer("Inferno")

	--Lightning
	game.ReplicatedStorage.rEvents.elementMasteryEvent:FireServer("Lightning")

	--Electral
	game.ReplicatedStorage.rEvents.elementMasteryEvent:FireServer("Electral Chaos")

	--Wrath
	game.ReplicatedStorage.rEvents.elementMasteryEvent:FireServer("Masterful Wrath")

	--Shadow
	game.ReplicatedStorage.rEvents.elementMasteryEvent:FireServer("Shadow Charge")

	--ShadowFire
	game.ReplicatedStorage.rEvents.elementMasteryEvent:FireServer("Shadowfire")

	--Eternity Storm
	game.ReplicatedStorage.rEvents.elementMasteryEvent:FireServer("Eternity Storm")
end)
