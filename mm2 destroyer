-- Gui to Lua
-- Version: 3.2

-- Instances:

local ScreenGui = Instance.new("ScreenGui")
local Gui = Instance.new("Frame")
local ImageLabel = Instance.new("ImageLabel")
local side = Instance.new("Frame")
local side_2 = Instance.new("Frame")
local Exit = Instance.new("ImageButton")
local Autofarm = Instance.new("TextButton")
local Fly = Instance.new("TextButton")
local TextLabel = Instance.new("TextLabel")

--Properties:

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Gui.Name = "Gui"
Gui.Parent = ScreenGui
Gui.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Gui.Position = UDim2.new(0.30320698, 0, 0.309202433, 0)
Gui.Size = UDim2.new(0, 539, 0, 263)
Gui.Active = true
Gui.Draggable = true

ImageLabel.Parent = Gui
ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ImageLabel.Position = UDim2.new(-0.00185528758, 0, 0, 0)
ImageLabel.Size = UDim2.new(0, 539, 0, 263)
ImageLabel.Image = "http://www.roblox.com/asset/?id=8606796112"

side.Name = "side"
side.Parent = Gui
side.BackgroundColor3 = Color3.fromRGB(85, 85, 85)
side.BorderColor3 = Color3.fromRGB(63, 63, 63)
side.Position = UDim2.new(0.864564002, 0, 0, 0)
side.Size = UDim2.new(0, 73, 0, 263)


side_2.Name = "side"
side_2.Parent = side
side_2.BackgroundColor3 = Color3.fromRGB(85, 85, 85)
side_2.BorderColor3 = Color3.fromRGB(63, 63, 63)
side_2.Position = UDim2.new(-6.38356161, 0, -0.155893534, 0)
